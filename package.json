{
  "name": "kmcg",
  "version": "1.0.0",
  "description": "Karen Mcgrane's blog and assorted migration tools",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">18.14.0"
  },
  "scripts": {
    "build": "npm run clean; npm run compile",
    "clean": "rm -rf ./dist;rm *.tsbuildinfo",
    "compile": "tsc -p ./tsconfig.build.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "author": "Jeff Eaton",
  "license": "MIT",
  "dependencies": {
    "@contentful/rich-text-from-markdown": "^15.16.6",
    "@salesforce/ts-types": "^2.0.3",
    "@types/locutus": "^0.0.6",
    "contentful": "^10.3.1",
    "contentful-management": "^10.38.3",
    "dot-prop": "^8.0.1",
    "fast-csv": "^4.3.6",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "locutus": "^2.0.16",
    "luxon": "^3.3.0",
    "node-glob": "^1.2.0",
    "sharp": "^0.32.1",
    "slug": "^8.2.2",
    "ts-dotenv": "^0.9.1",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.2.5",
    "@types/slug": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
